<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vremenskaprognoza.Pregled.PregledController">
   <children>
      <Label layoutX="60.0" layoutY="78.0" text="Grad:" />
      <Label layoutX="60.0" layoutY="104.0" text="Ulica:" />
      <Button fx:id="idPrikaziButton" layoutX="109.0" layoutY="269.0" mnemonicParsing="false" onAction="#clickedPrikaziButton" prefHeight="58.0" prefWidth="150.0" text="Prikazi" />
      <Label layoutX="54.0" layoutY="53.0" text="Drzava:" />
      <Label layoutX="10.0" layoutY="130.0" text="Datum i vrijeme:" />
      <ComboBox fx:id="idDrzava" layoutX="101.0" layoutY="49.0" onAction="#selectedDrzava" prefWidth="150.0" />
      <ComboBox fx:id="idGrad" layoutX="101.0" layoutY="74.0" onAction="#selectedGrad" prefWidth="150.0" />
      <ComboBox fx:id="idUlica" layoutX="101.0" layoutY="100.0" onAction="#selectedUlica" prefWidth="150.0" />
      <ComboBox fx:id="idDatumVrijeme" layoutX="101.0" layoutY="126.0" onAction="#selectedDatumVrijeme" prefWidth="150.0" />
   </children>
</AnchorPane>
